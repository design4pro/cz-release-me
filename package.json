{
  "name": "cz-release-me",
  "version": "1.6.1",
  "description": "A customizable commitizen adapter for conventions",
  "bin": "index.js",
  "main": "index.js",
  "files": [
    "lib",
    "CHANGELOG.md",
    "index.js",
    "LICENCE.md",
    "README.md"
  ],
  "scripts": {
    "release": "release-me --release-count=0",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test": "nyc --reporter=html --reporter=text mocha --timeout 20000 --reporter spec $(find . -name '*test.js' ! -ipath '*node_modules*')",
    "test:check-coverage": "nyc check-coverage --statements 75 --branches 65 --functions 80 --lines 75",
    "test:debug": "DEBUG=mocha npm t",
    "test:watch": "npm t -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/design4pro/cz-release-me.git"
  },
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "changelog",
    "conventions",
    "cz"
  ],
  "author": {
    "name": "Rafa≈Ç Wolak",
    "email": "r.wolak@design4.pro",
    "url": "https://www.design4.pro"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/design4pro/cz-release-me/issues/"
  },
  "homepage": "https://github.com/design4pro/cz-release-me#readme",
  "dependencies": {
    "chalk": "^2.0.0",
    "editor": "^1.0.0",
    "findup": "^0.1.5",
    "inquirer": "^6.0.0",
    "temp": "^0.8.3",
    "winston": "^2.3.1",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chai-spies": "1.0.0",
    "codecov": "3.1.0",
    "commitizen": "2.10.1",
    "debug": "4.0.1",
    "git-dummy-commit": "1.3.0",
    "istanbul": "0.4.5",
    "jscs": "3.0.7",
    "jshint": "2.9.6",
    "mocha": "5.2.0",
    "mock-git": "1.0.3",
    "nyc": "11.9.0",
    "release-me": "1.4.2",
    "rewire": "4.0.1",
    "rimraf": "2.6.2",
    "shelljs": "0.8.2",
    "sinon": "5.1.1",
    "sinon-chai": "3.2.0",
    "standard": "11.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./index.js"
    }
  }
}
