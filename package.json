{
  "name": "cz-release-me",
  "version": "1.1.0",
  "description": "A customizable commitizen adapter for conventions",
  "bin": "dist/index.js",
  "scripts": {
    "build": "babel --copy-files --out-dir dist src",
    "check-coverage": "nyc check-coverage --statements 93 --branches 75 --functions 95 --lines 95",
    "cover": "nyc --reporter=lcov npm t",
    "prebuild": "rimraf dist",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "release": "release-me",
    "start": "npm run test",
    "test": "mocha --timeout 15000 test/index.js --compilers js:babel-register",
    "test:debug": "DEBUG=mocha npm t",
    "validate": "npm run cover && npm run check-coverage",
    "watch:test": "npm t -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/design4pro/cz-release-me.git"
  },
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "changelog",
    "conventions",
    "cz"
  ],
  "author": {
    "name": "Rafa≈Ç Wolak",
    "email": "r.wolak@design4.pro",
    "url": "https://www.design4.pro"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/design4pro/cz-release-me/issues/"
  },
  "homepage": "https://github.com/design4pro/cz-release-me#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "editor": "1.0.0",
    "findup": "^0.1.5",
    "inquirer": "1.2.3",
    "temp": "0.8.3",
    "winston": "2.1.0",
    "word-wrap": "1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "chai-spies": "^0.7.1",
    "codecov": "^2.2.0",
    "commitizen": "^2.9.6",
    "debug": "^2.6.8",
    "git-dummy-commit": "^1.3.0",
    "istanbul": "^0.4.5",
    "jscs": "^3.0.7",
    "jshint": "^2.9.4",
    "mocha": "^3.4.2",
    "mock-git": "^1.0.2",
    "nyc": "^10.3.2",
    "release-me": "^1.1.1",
    "rewire": "^2.5.2",
    "rimraf": "^2.6.1",
    "shelljs": "^0.7.3",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0",
    "standard": "^10.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./dist/index.js"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
